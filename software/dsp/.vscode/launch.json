{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./bin/v1.icyradio-dsp.elf",
            "request": "launch",
            "type": "cortex-debug",
            "preLaunchTask": "Build",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/.vscode/openocd-scripts/target.cfg"
            ],
            "device": "ATSAMV71Q21B",
            "svdFile": "${env:ARM_LIBROOT}/Atmel.SAMV71_DFP.2.4.182/samv71b/svd/ATSAMV71Q21B.svd",
            "interface": "swd",
            "swoConfig": {
                "enabled": true,
                "swoFrequency": 12000000,
                "cpuFrequency": 360000000,
                "source": "serial",
                "swoPath": "/dev/ttyUSB1",
                "decoders": [
                    {
                        "port": 0,
                        "label": "Console",
                        "type": "console",
                        "showOnStartup": true
                    },
                ]
            }
        }
    ]
}